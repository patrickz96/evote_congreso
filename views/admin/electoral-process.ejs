function get_electoral_process(){
  
  $.get("/admin/electoral-process/all", function(processes, status){
    
    var table = $('#table_process');
    //table.find("tbody tr").remove();
    var i=1
    processes.forEach(function (process) {
        table.append("<tr><td>" + i + "</td><td>" + process.name + "</td><td>"+ process.date_init  +"</td><td>"+ process.date_end+"</td></tr>");
        i++;
    });
    
    $('#table_process').DataTable();
    //alert("Data: " + data + "\nStatus: " + status);
  });
} 

var v_8digits = false;
function validateForm(){
  return true;
}

</script>



</body>
</html>




