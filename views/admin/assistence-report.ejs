function load_asistencia_departamento_academico(){

  $.get("/admin/asistencia-departamento", function(asistencias, status){
    var table = $('#tabla_asistencia_dp');
    var i=1;
    var total_habilitados = 0;
    var total_asistentes = 0;
    asistencias.forEach(function (asistencia) {
      table.append("<tr><td>" + i + "</td><td>" + asistencia.nombre + "</td><td>"+ asistencia.habilitados + "</td><td>"+ asistencia.asistentes +"</td><td>"+ asistencia.porcentaje+'%' +"</td></tr>");
      total_habilitados+=parseInt(asistencia.habilitados);
      total_asistentes+=parseInt(asistencia.asistentes);
      i++;
    });
    if(total_asistentes!=0){
      total_porcentaje = Math.round((total_asistentes*100)/total_habilitados,0); 
    }else{
      total_porcentaje =0;
    }
    table.append("<tr><td>" + "</td><td> TOTAL </td><td>"+ total_habilitados + "</td><td>"+ total_asistentes +"</td><td>"+ total_porcentaje+'%' +"</td></tr>");
    
    table.tableExport({
      formats: ["xlsx","txt", "csv"], //Tipo de archivos a exportar ("xlsx","txt", "csv", "xls")
      position: 'top',  // Posicion que se muestran los botones puedes ser: (top, bottom)
      bootstrap: false,//Usar lo estilos de css de bootstrap para los botones (true, false)
      fileName: "Asistencia-DepartamentoAcademico",    //Nombre del archivo 
    });
  
  });

}


