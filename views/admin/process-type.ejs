 function get_electoral_process(){
  
  $.get("/admin/electoral-process/all", function(processes, status){
    
    var select_process = $('#electoral_process');
    processes.forEach(function (process) {
        var option = new Option(process.name, process.id_electoral_process);
        $(option).html(process.name);
        select_process.append(option);
    });
  });
} 

function get_processes_type(){
  
  $.get("/admin/process-type/all", function(processes_type, status){
    var table = $('#table_process-type');
    //table.find("tbody tr").remove();
    var i=1;
    processes_type.forEach(function (process_type) {

        table.append("<tr><td>" + i + "</td><td>" + process_type.electoral_process.name + "</td><td>"+ process_type.name  +"</td></tr>");
        i++;
    });
    
    $('#table_process-type').DataTable();
    //alert("Data: " + data + "\nStatus: " + status);
  });
} 

function validateForm(){
  return true;
}

